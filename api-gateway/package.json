{
  "name": "api-gateway",
  "version": "1.0.0",
  "description": "GraphQL API Gateway",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint --max-warnings=0 '{src,__tests__}/**/*.ts'",
    "lint:fix": "eslint --fix '{src,__tests__}/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "generate:typings": "ts-node ./src/graphql/generate-typings",
    "copy:protos": "cpy ../_proto ./src/_proto"
  },
  "dependencies": {
    "@grpc/proto-loader": "0.5.4",
    "@nestjs/common": "7.1.1",
    "@nestjs/config": "0.5.0",
